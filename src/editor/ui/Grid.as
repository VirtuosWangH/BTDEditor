package editor.ui{		import flash.display.*	import flash.events.*;		public class Grid extends Sprite {				public function Grid(){			mouseEnabled = false;			mouseChildren= false;			addEventListener(Event.ADDED_TO_STAGE, onAdded);		}		private function onAdded(evt:Event):void{			build();			removeEventListener(Event.ADDED_TO_STAGE, onAdded);		}		public function build():void{			graphics.clear();						for (var i:int = 0; i<stage.stageWidth; i+=10){				graphics.lineStyle(0, 0xCCCCCC);				if (i % 30 == 0){					graphics.lineStyle(1, 0x666666);				}				graphics.moveTo(i, 0);				graphics.lineTo(i, stage.stageHeight);							}			for (i = 0; i<stage.stageHeight; i+=10){				graphics.lineStyle(0, 0xCCCCCC);				if (i % 30 == 0){					graphics.lineStyle(1, 0x666666);				}				graphics.moveTo(0, i);				graphics.lineTo(stage.stageWidth, i);			}		}			}}